{
  "name": "yarems",
  "version": "0.0.1",
  "description": "Yet Another React Electron Menubar Starter",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=development electron .",
    "dev": "concurrently -k 'babel-node server.js' 'npm start'",
    "build": "NODE_ENV=production webpack --config webpack.config.production.babel.js && electron-packager . Yarems --platform=darwin --arch=all --prune --out release --overwrite --icon ./app.icns"
  },
  "author": "Emmanuel Pilande",
  "engine-strict": true,
  "engines": {
    "node": ">= 6.x",
    "npm": ">= 3.x"
  },
  "dependencies": {
    "@babel/core": "^7.0.0-beta.38",
    "@babel/node": "^7.0.0-beta.38",
    "@babel/polyfill": "^7.0.0-beta.38",
    "@babel/preset-env": "^7.0.0-beta.38",
    "@babel/preset-react": "^7.0.0-beta.38",
    "@babel/preset-stage-0": "^7.0.0-beta.38",
    "@babel/register": "^7.0.0-beta.38",
    "babel-eslint": "^8.0.2",
    "babel-loader": "^8.0.0-beta.0",
    "electron": "^1.4.7",
    "electron-debug": "^1.1.0",
    "electron-devtools-installer": "^2.0.1",
    "electron-packager": "^10.1.2",
    "fix-path": "^2.1.0",
    "menubar": "^5.1.0",
    "prop-types": "^15.6.0",
    "react": "^16.1.0",
    "react-dom": "^16.1.0",
    "react-hot-loader": "^4.0.0-beta.18",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "styled-components": "^3.0.2"
  },
  "devDependencies": {
    "babel-plugin-styled-components": "^1.5.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "concurrently": "^3.1.0",
    "devtron": "^1.4.0",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "express": "^4.14.0",
    "file-loader": "^1.1.5",
    "stylelint": "^8.2.0",
