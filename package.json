{
  "name": "yarems",
  "version": "0.0.1",
  "description": "Yet Another React Electron Menubar Starter",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=development electron .",
    "dev": "concurrently -k 'node -r babel-register server.js' 'npm start'",
    "build": "NODE_ENV=production webpack --config webpack.config.production.babel.js && electron-packager . Yarems --platform=darwin --arch=all --prune --out release --overwrite --icon ./app.icns"
  },
  "author": "Emmanuel Pilande",
  "engine-strict": true,
  "engines": {
    "node": ">= 6.x",
    "npm": ">= 3.x"
  },
  "dependencies": {
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.7",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "classnames": "^2.2.5",
    "electron": "^1.4.7",
    "electron-debug": "^1.1.0",
    "electron-devtools-installer": "^2.0.1",
    "electron-packager": "^8.3.0",
    "fix-path": "^2.1.0",
    "menubar": "^5.1.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "sanitize.css": "^4.1.0"
  },
  "devDependencies": {
    "concurrently": "^3.1.0",
    "css-loader": "^0.26.1",
    "devtron": "^1.4.0",
    "eslint": "^3.10.0",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.6.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.10.0",
    "postcss-cssnext": "^2.8.0",
    "postcss-import": "9.1.0",
    "postcss-loader": "^1.1.1",
    "postcss-reporter": "^3.0.0",
    "style-loader": "^0.13.1",
    "stylelint": "^7.6.0",
    "stylelint-config-standard": "^15.0.0",
    "webpack": "^2.1.0-beta.25",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.2",
    "webpack-merge": "^2.6.1"
  }
}
